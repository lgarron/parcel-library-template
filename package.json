{
  "name": "parcel-library-template",
  "version": "0.0.1",
  "description": "",
  "types": "dist/template.d.ts",
  "main": "dist/template.node.js",
  "module": "dist/template.module.js",
  "browser": "dist/template.browser.js",
  "scripts": {
    "build": "npm run build-types && npm run build-main && npm run build-module && npm run build-browser",
    "build-types": "npx parcel build --target types src/targets/types.ts",
    "build-main": "npx parcel build --target main src/targets/node.ts",
    "build-module": "npx parcel build --target module src/targets/module.ts",
    "build-browser": "npx parcel build --target browser src/targets/browser.ts",
    "dev": "npx parcel serve src/targets/dev/index.html",
    "clean": "rm -rf ./.parcel-cache ./dist",
    "prepack": "npm run clean && npm run build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cubing": "^0.10.5"
  },
  "devDependencies": {
    "parcel": "^2.0.0-nightly.288"
  },
  "targets": {
    "module": {},
    "main": {
      "context": "node"
    },
    "browser": {
      "includeNodeModules": true
    }
  }
}
