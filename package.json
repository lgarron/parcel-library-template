{
  "name": "parcel-library-template",
  "version": "0.0.1",
  "description": "",
  "module": "dist/template.module.js",
  "types": "dist/template.d.ts",
  "browser": "dist/template.browser.js",
  "scripts": {
    "build": "npm run types && npm run module && npm run browser",
    "module": "npx parcel build --target module src/targets/module.ts",
    "types": "npx parcel build --target types src/targets/types.ts",
    "browser": "npx parcel build --target browser src/targets/browser.ts",
    "dev": "npx parcel serve src/targets/dev/index.html",
    "clean": "rm -rf ./.parcel-cache ./dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cubing": "^0.10.5"
  },
  "devDependencies": {
    "parcel": "^2.0.0-nightly.288"
  },
  "targets": {
    "module": {},
    "browser": {
      "includeNodeModules": true
    }
  }
}
